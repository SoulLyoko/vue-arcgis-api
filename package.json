{
  "name": "vue-arcgis-api",
  "version": "0.0.13",
  "private": false,
  "description": "vue-arcgis-api",
  "keyword": [
    "vue",
    "component",
    "arcgis",
    "arcgis-api"
  ],
  "author": "Lyoko <519052532@qq.com>",
  "license": "MIT",
  "main": "packages/index.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build --target lib --name vue-arcgis --dest lib packages/index.js",
    "arcgis:cp": "ncp ./node_modules/@arcgis/core/assets ./public/assets",
    "docs:cp": "ncp ./node_modules/@arcgis/core/assets ./docs/.vuepress/public/assets",
    "docs:dev": "npm run docs:cp && vuepress dev docs",
    "docs:build": "npm run docs:cp && vuepress build docs"
  },
  "dependencies": {
    "core-js": "^3.18.2",
    "mitt": "^3.0.0",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@arcgis/core": "^4.21.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.19.1",
    "lint-staged": "^11.2.1",
    "ncp": "^2.0.0",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "sass-loader": "^10.2.0",
    "vue-template-compiler": "^2.6.14",
    "vuepress": "^1.8.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}

{
  "name": "vue-arcgis-api",
  "version": "0.1.2",
  "description": "vue-arcgis-api",
  "author": "SoulLyoko <519052532@qq.com>",
  "private": false,
  "license": "MIT",
  "main": "lib/index.umd.js",
  "keywords": [],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "docs:dev": "npm run cp && vitepress dev docs",
    "docs:build": "npm run fix-memory-limit && npm run cp && vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint --ext .ts,.vue .",
    "prepare": "husky install",
    "fix": "vue-demi-fix",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "cp:readme": "ncp ./README.md ./docs/index.md",
    "cp:log": "ncp ./CHANGELOG.md ./docs/CHANGELOG.md",
    "cp:arcgis": "ncp ./node_modules/@arcgis/core/assets ./docs/public/assets",
    "cp": "npm run cp:readme && npm run cp:log"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "vue-demi": "^0.12.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.3.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arcgis/core": "^4.21.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "3.2.20",
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "^7.0.4",
    "increase-memory-limit": "^1.0.7",
    "lint-staged": "^11.2.6",
    "ncp": "^2.0.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.13",
    "vitepress": "^0.20.0",
    "vue": "^3.2.20",
    "vue-dts-gen": "^0.3.0",
    "vue-tsc": "^0.3.0",
    "vue2": "npm:vue@2"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}

{
  "name": "vue-arcgis-api",
  "version": "0.2.1",
  "description": "vue-arcgis-api",
  "author": "SoulLyoko <519052532@qq.com>",
  "repository": "https://github.com/SoulLyoko/vue-arcgis-api.git",
  "homepage": "https://github.com/SoulLyoko/vue-arcgis-api#readme",
  "private": false,
  "license": "MIT",
  "main": "lib/vue-arcgis-api.umd.js",
  "types": "lib/vue-arcgis-api.d.ts",
  "keywords": [
    "vue",
    "arcgis",
    "arcgis-api"
  ],
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev2": "node scripts/dev2.js",
    "build": "node scripts/build.js",
    "serve": "vite preview",
    "docs:dev": "node scripts/docs_dev.js",
    "docs:build": "node scripts/docs_build.js",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint --ext .ts,.vue .",
    "prepare": "husky install",
    "precommit": "node scripts/precommit.js",
    "release": "node scripts/release.js",
    "deploy": "node scripts/deploy.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cp:arcgis": "ncp ./node_modules/@arcgis/core/assets ./docs/public/assets"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "vue-demi": "^0.12.1"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.3",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arcgis/core": "~4.22.2",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "3.2.26",
    "@vue/composition-api": "^1.4.3",
    "@vue/runtime-dom": "^3.2.26",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "enquirer": "^2.3.6",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "execa": "^4.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "ncp": "^2.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-vue2": "^1.9.2",
    "vitepress": "^0.21.3",
    "vue": "^3.2.26",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.30.2"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
